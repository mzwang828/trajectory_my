<?xml version="1.0"?>
<robot name="boxx">

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <link name="box">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.1666667" ixy="0.0" ixz="0.0" iyy="0.1666667" iyz="0.0" izz="0.1666667"/>
    </inertial>
  </link>

  <link name="obj_front">
    <visual>
      <geometry>
        <sphere radius="1e-6"/>
      </geometry>
    <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="1e-6"/>
      </geometry>
    </collision>
  </link>

  <joint name="obj_front_link" type="fixed">
    <parent link="box"/>
    <child link="obj_front"/>
    <origin xyz="-0.025 0 0"/>
  </joint>
  
</robot>
